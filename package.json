{
    "name": "nest-mongodb",
    "version": "1.0.0",
    "description": "A NestJS module for the MongoDB driver",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "npm run build:clean && npm run build:tsc",
        "build:clean": "rimraf dist",
        "build:tsc": "tsc",
        "format:check": "prettier --list-different \"src/**/*.ts\"",
        "format:write": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "mongo",
        "mongodb",
        "nest",
        "nestjs"
    ],
    "author": "Erik Christensen",
    "license": "ISC",
    "devDependencies": {
        "@nestjs/common": "^5.5.0",
        "@nestjs/core": "^5.5.0",
        "@types/mongodb": "^3.1.18",
        "@types/node": "^10.12.18",
        "@types/rimraf": "^2.0.2",
        "mongodb": "^3.1.10",
        "prettier": "^1.15.3",
        "reflect-metadata": "^0.1.12",
        "rimraf": "^2.6.3",
        "rxjs": "^6.3.3",
        "typescript": "^3.2.2"
    },
    "peerDependencies": {
        "@nestjs/common": "^5.1.0",
        "@nestjs/core": "^5.1.0",
        "mongodb": "^3.1.0",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/erikc5000/nest-mongodb.git"
    }
}
